{
    "name": "aiida-nims-scheduler",
    "author": "Atsushi Togo",
    "author_email": "atz.togo@gmail.com",
    "description": "NIMS supercomputer scheduler",
    "url": "https://github.com/atztogo/aiida-nims-scheduler",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.schedulers": [
            "pbs_nims = aiida_nims_scheduler.schedulers.pbs_nims:PbsNimsScheduler"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.2.1,<2.0.0",
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid==2.2.5",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint==2.3.1"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
